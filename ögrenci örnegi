#include <stdio.h>
#include <stdlib.h>
#include <math.h>
void bastir(int matris[][4], int size) {
	int i, j;
	for (i = 0; i < size; i++) {
		for (j = 0; j < 4; j++) {
			printf("matrisimiz", matris[i][j]);
		}
	}


}

int main()
{
	int matris[3][4] = { { 77,68,86,73 },{ 96,87,89,78 },{ 70,90,86,81 } };


	bastir(matris, 3);

	system("pause");
	return 0;
}
