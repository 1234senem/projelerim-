#include<stdio.h>
#include<stdlib.h>
int asal_mi(int sayi) {
	
	int i;

for (i = 0; i < sayi; i++){

			if (sayi %i == 0) {
				return 0;
			}

			//sayi asal degilse tekrar kontol etmeye gerek yok 

		}
		return 1;
	
}


int main() {
	int n;
	printf("bir sayi giriniz:");
	scanf("%d", &n);
	if (asal_mi(n) == 0) {
		printf("sayi asal degil");
	}
	else {
		printf("sayi asaldÄ±r.");
	}


	return 0;
	
	system("pause");
	getch();

}
