#include<stdio.h>
#include<conio.h>

int main() {

	int a[100], i, x = 0, toplam = 0;
	printf("ilk eleman giriniz:  "); scanf("%d", a[0]);
	for (i = 0; i < 10; i++) {

		if (a[i] != -1) {
			printf("diziye elaman giriniz:"); scanf("%d", &a[i + 1]);
			x++;

		}
		else {

			toplam = x/2;
			printf("ortadaki eleman=%d", a[toplam]);
			break;
		}



	}


	getchar();
	return 0;
}
