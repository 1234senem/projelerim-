#include<stdio.h>
#include<stdlib.h>
//2 boyutlu matris de zorunlu 2.boyut ve size
void bastir(int matris[][4], int size) {
	int i, j;
	for (i = 0; i < size; i++) {
		for (j = 0; j < 4; j++) {
			printf("%d", matris[i][j]);
		}
	}


}

int main() {

	int matris[3][4];
	int i, j;
	printf("matrisi doldurunuz");
	for (i = 0; i < 3; i++) {
		for(j = 0; j < 4;j++){
			scanf("%d", &matris[i][j]);
		
		}
		printf("\n");
	}
	bastir(matris, 3);//satÄ±r boyutu zorunlu
	system("pause");
	return 0;



}
