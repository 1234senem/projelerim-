#include<stdio.h>
#include<math.h>
int main() {
    float a,b,c;
    float delta;
    float x1,x2;
    printf("ax^2+bx+c=0 ikinci dereceden denklemin\n");
    printf("katsayilarini giriniz:");
    scanf("%f %f %f",&a,&b,&c);
    delta=b*b-4*a*c;
    if(delta>=0)   {
    x1=(-b-sqrt(delta))/(2*a);
    x2=(-b+sqrt(delta))/(2*a);
    printf("x1=%f\n",x1);
    printf("x2=%f\n",x2);
}
else printf("reel kok yoktur...");getchar();getchar();
return 0; }
