#include<stdio.h>
#include<string.h>
#include<stdlib.h>

int uzunlukdön(char name[]) {
	int uzunluk = 0;
	int i;
	for (i = 0; name[i] != '\0'; i++) {
		uzunluk++;

	}
	return uzunluk;
}

int main() {

	char isim[] = "sinemjklf dsjagıjfgıoajıopgs";
	printf("%d", uzunlukdön(isim));

	system("pause");
	return 0;
}
